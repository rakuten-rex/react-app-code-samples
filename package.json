{
  "name": "react-app-code-samples",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rakuten-rex/button": "^1.3.0",
    "@rakuten-rex/checkbox": "^1.3.0",
    "@rakuten-rex/grid": "^1.5.0",
    "@rakuten-rex/panel": "^1.1.0",
    "@rakuten-rex/password": "^1.2.0",
    "@rakuten-rex/radio-button": "^1.3.0",
    "@rakuten-rex/select": "^1.3.2",
    "@rakuten-rex/stepper": "^1.5.0",
    "@rakuten-rex/switch": "^1.2.0",
    "@rakuten-rex/text-field": "^1.2.0",
    "@rakuten-rex/textarea": "^1.2.1",
    "@rakuten-rex/track": "^1.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "PUBLIC_URL=/react-app-code-samples/ react-scripts start",
    "build": "PUBLIC_URL=/react-app-code-samples/ react-scripts build",
    "postbuild": "PUBLIC_URL=/react-app-code-samples/ react-snap && npm run buid:docs",
    "buid:docs": "rm -rf docs && mv build docs && mkdir -p build/react-app-code-samples && cp -R docs/* build/react-app-code-samples",
    "serve": "serve -s build",
    "test": "PUBLIC_URL=/react-app-code-samples/ react-scripts test",
    "eject": "PUBLIC_URL=/react-app-code-samples/ react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "react-router-dom": "^5.1.2",
    "react-snap": "^1.23.0",
    "typescript": "^3.8.3"
  }
}